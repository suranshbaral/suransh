{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@components/*": [
                "src/components/*"
            ],
            "@constants/*": [
                "src/constants/*"
            ],
            "@hoc/*": [
                "src/hoc/*"
            ],
            "@utils/*": [
                "src/utils/*"
            ],
            "@/*": [
                "src/*"
            ],
            "@build/*": [
                "src/build/*"
            ],
            "@assets/*": [
                "src/assets/*"
            ],
            "@desktop_pc/*": [
                "src/build/desktop_pc/*"
            ],
            "@planet/*": [
                "src/build/planet/*"
            ],
            "~assets/*": [
                "src/assets/*"
            ],
            "~build/*": [
                "src/build/*"
            ],
            "~components/*": [
                "src/components/*"
            ],
            "~constants/*": [
                "src/constants/*"
            ],
            "~hoc/*": [
                "src/hoc/*"
            ],
            "~utils/*": [
                "src/utils/*"
            ],
            "~/*": [
                "src/*"
            ]
        }
    }
}